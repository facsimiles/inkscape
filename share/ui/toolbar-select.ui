<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkBox" id="select-toolbar">
    <property name="halign">start</property>
    <property name="valign">center</property>
    <property name="hexpand">True</property>
    <child>
      <object class="GtkButton">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Select all objects</property>
        <property name="action-name">win.select-all</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image1">
            <property name="icon-name">edit-select-all</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Select all in all layers</property>
        <property name="action-name">win.select-all-layers</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image2">
            <property name="icon-name">edit-select-all-layers</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="deselect_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Deselect any selected objects</property>
        <property name="action-name">win.select-none</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image3">
            <property name="icon-name">edit-select-none</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkToggleButton" id="_select_touch_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Toggle selection box to select all touched objects</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image4">
            <property name="icon-name">selection-touch</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkSeparator">
        <style>
            <class name="priority1"/>
        </style>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="rotate_left_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Rotate selection 90\xc2\xb0 counter-clockwise</property>
        <property name="action-name">app.object-rotate-90-ccw</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <style>
          <class name="context_item"/>
        </style>
        <child>
          <object class="GtkImage" id="image5">
            <property name="icon-name">object-rotate-left</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="rotate_right_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Rotate selection 90\xc2\xb0 clockwise</property>
        <property name="action-name">app.object-rotate-90-cw</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <style>
          <class name="context_item"/>
        </style>
        <child>
          <object class="GtkImage" id="image6">
            <property name="icon-name">object-rotate-right</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="flip_horizontal_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Flip selected objects horizontally</property>
        <property name="action-name">app.object-flip-horizontal</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <style>
          <class name="context_item"/>
        </style>
        <child>
          <object class="GtkImage" id="image7">
            <property name="icon-name">object-flip-horizontal</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="flip_vertical_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Flip selected objects vertically</property>
        <property name="action-name">app.object-flip-vertical</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <style>
          <class name="context_item"/>
        </style>
        <child>
          <object class="GtkImage" id="image8">
            <property name="icon-name">object-flip-vertical</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkSeparator">
        <style>
            <class name="priority1"/>
        </style>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="raise_to_top_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Raise selection to top</property>
        <property name="action-name">app.selection-top</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image9">
            <property name="icon-name">selection-top</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="raise_one_step_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Raise selection one step</property>
        <property name="action-name">app.selection-raise</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image10">
            <property name="icon-name">selection-raise</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="lower_one_step_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Lower selection one step</property>
        <property name="action-name">app.selection-lower</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image11">
            <property name="icon-name">selection-lower</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="lower_to_bottom_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Lower selection to bottom</property>
        <property name="action-name">app.selection-bottom</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image12">
            <property name="icon-name">selection-bottom</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkSeparator">
        <style>
            <class name="priority1"/>
        </style>
      </object>
    </child>
    <child>
      <object class="GtkToggleButton" id="_transform_stroke_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">When scaling objects, scale the stroke width by the same proportion</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image13">
            <property name="icon-name">transform-affect-stroke</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkToggleButton" id="_transform_corners_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">When scaling rectangles, scale the radii of rounded corners</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image14">
            <property name="icon-name">transform-affect-rounded-corners</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkToggleButton" id="_transform_gradient_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Move gradients (in fill or stroke) along with the objects</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image15">
            <property name="icon-name">transform-affect-gradient</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkToggleButton" id="_transform_pattern_btn">
        <property name="valign">center</property>
        <property name="focusable">True</property>
        <property name="tooltip-text" translatable="yes">Move patterns (in fill or stroke) along with the objects</property>
        <property name="has-frame">False</property>
        <style>
            <class name="priority1"/>
        </style>
        <child>
          <object class="GtkImage" id="image16">
            <property name="icon-name">transform-affect-pattern</property>
          </object>
        </child>
      </object>
    </child>
    <style>
      <class name="inkscape-toolbar"/>
    </style>
  </object>
</interface>
